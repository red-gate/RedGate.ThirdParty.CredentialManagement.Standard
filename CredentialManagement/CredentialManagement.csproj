<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <AssemblyName>RedGate.ThirdParty.CredentialManagement.Standard</AssemblyName>
        <SignAssembly>true</SignAssembly>
        <AssemblyOriginatorKeyFile>..\RedGate.snk</AssemblyOriginatorKeyFile>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>RedGate.ThirdParty.CredentialManagement.Standard</Title>
        <Authors>Red Gate Software Ltd</Authors>
        <Description>A customized CredentialManagement.Standard library for cross-compatibility of our products</Description>
        <Copyright>Copyright © Red Gate Software Ltd 2019</Copyright>
        <PackageProjectUrl>https://github.com/red-gate/Redgate.ThirdParty.CredentialManagement.Standard</PackageProjectUrl>
        <RepositoryUrl>https://github.com/red-gate/Redgate.ThirdParty.CredentialManagement.Standard</RepositoryUrl>
        <PackageVersion>1.0.5</PackageVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="System.Security.Permissions" Version="5.0.0"/>
    </ItemGroup>

</Project>
